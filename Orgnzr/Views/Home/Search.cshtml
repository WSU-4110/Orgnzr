@{
    ViewData["Title"] = "Search";
}
<h1>@ViewData["Title"]</h1>

<div class="wrap">
    <div class="search">
        <input type="text" class="searchTerm" placeholder="Enter a key phrase to search">
        <button type="submit" class="searchButton">
            <i class="fa fa-search"></i>
        </button>
    </div>
</div>